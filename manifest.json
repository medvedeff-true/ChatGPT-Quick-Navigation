{
  "manifest_version": 3,
  "name": "ChatGPT Quick Navigation",
  "version": "1.0.0",
  "description": "Быстрая навигация по вопросам пользователя в текущем чате на chatgpt.com. ⠀⠀⠀⠀⠀⠀⠀⠀ Автор - Medvedeff ⠀⠀⠀⠀⠀⠀⠀⠀⠀⠀⠀⠀⠀⠀⠀⠀⠀⠀⠀⠀ github.com/medvedeff-true",
  "host_permissions": ["https://chatgpt.com/*"],
  "icons": {
    "16": "icons/16x16.png",
    "24": "icons/24x24.png",
    "32": "icons/32x32.png",
    "48": "icons/48x48.png",
    "128": "icons/128x128.png"
  },
  "action": {
    "default_icon": {
      "16": "icons/16x16.png",
      "24": "icons/24x24.png",
      "32": "icons/32x32.png",
      "48": "icons/48x48.png",
      "128": "icons/128x128.png"
    }
  },
  "content_scripts": [
    {
      "matches": ["https://chatgpt.com/*"],
      "js": ["content.js"],
      "css": ["styles.css"],
      "run_at": "document_idle"
    }
  ]
}
